.btn-null{ margin: 0; padding: 0; border-radius: 0; border: none; line-height: normal; background: none; }
.btn-null::after{ border: none; }
.btn{ padding: 9rpx 0; border-radius: 5px; font-size: 24rpx; line-height: 23px; margin-bottom: 22rpx; display: -webkit-flex; display: flex; -webkit-align-items: center; align-items: center; }
.btn.btn-green{ background-color: #b4ffd2; box-shadow: 0 2px 0 0 #83e8ab; color: #000; }
.btn.btn-green3{ background-color: #00ff66; box-shadow: 0 2px 0 0 #00d254; color: #000; }
.btn:active{ background-color: #f8f8f8; }
.btn.btn-green:active{ background-color: #83e8ab; }
.btn.btn-green3:active{ background-color: #00d254; }

 
